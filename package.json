{
    "name": "igrounding",
    "private": true,
    "version": "1.0.4",
    "type": "module",
    "author": "Michael Poulgrain",
    "repository": {
        "type": "git",
        "url": "https://github.com/Michaelp1994/transmission-lines"
    },
    "scripts": {
        "bump": "npm version patch && git push --follow-tags",
        "build": "turbo run build",
        "package": "turbo run package",
        "clean": "turbo run clean && rm -rf .turbo node_modules dist releases",
        "db:push": "turbo run db:push",
        "dev": "turbo run dev",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "lint": "turbo run lint --continue",
        "lint:fix": "turbo run lint:fix --continue",
        "test": "turbo run test",
        "typecheck": "turbo run typecheck",
        "generate": "turbo run generate",
        "prepare": "husky",
        "commitlint": "commitlint --edit"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "commitizen": "^4.3.1",
        "commitlint": "^19.5.0",
        "husky": "^9.1.6",
        "prettier": "^3.3.3",
        "semantic-release": "^24.1.3",
        "turbo": "^2.2.3"
    },
    "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
    "pnpm": {
        "patchedDependencies": {
            "electron-trpc": "patches/electron-trpc.patch"
        }
    },
    "dependencies": {
        "@changesets/cli": "^2.27.9"
    }
}
